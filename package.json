{
  "name": "nintendo-switch-eshop",
  "version": "6.0.1",
  "description": "Unofficial API lib for Nintendo Switch eShop game listing and pricing information.",
  "author": "lmmfranco",
  "license": "Apache-2.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    "import": "./dist/index.mjs",
    "require": "./dist/index.js"
  },
  "scripts": {
    "test": "jest",
    "lint": "eslint src __tests__ --ext js,ts --fix",
    "build": "tsc -b src",
    "postbuild": "gen-esm-wrapper dist/index.js dist/index.mjs",
    "watch": "tsc -b src -w",
    "clean": "tsc -b src --clean",
    "docs": "typedoc",
    "sversion": "standard-version",
    "commit": "git-cz",
    "cz": "git-cz",
    "update": "yarn upgrade-interactive --latest",
    "prepublishOnly": "yarn build",
    "prepare": "husky install .github/husky"
  },
  "dependencies": {
    "@types/country-data": "^0.0.2",
    "country-data": "^0.0.31",
    "fast-xml-parser": "^3.20.3",
    "node-fetch": "^3.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.2.1",
    "@commitlint/config-conventional": "^13.2.0",
    "@sapphire/eslint-config": "^4.0.0",
    "@sapphire/prettier-config": "^1.2.2",
    "@sapphire/ts-config": "^3.1.1",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.10.3",
    "@types/node-fetch": "^3.0.3",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "common-tags": "^1.8.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "gen-esm-wrapper": "^1.1.3",
    "husky": "^7.0.2",
    "jest": "^27.2.5",
    "jest-circus": "^27.2.5",
    "lint-staged": "^11.2.3",
    "prettier": "^2.4.1",
    "pretty-quick": "^3.1.1",
    "replace": "^1.2.1",
    "standard-version": "^9.3.1",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.3.0",
    "typedoc": "^0.22.5",
    "typedoc-plugin-nojekyll": "^1.0.1",
    "typescript": "^4.4.3"
  },
  "files": [
    "dist/",
    "!dist/*.tsbuildinfo"
  ],
  "engines": {
    "node": ">=10",
    "npm": ">=6"
  },
  "keywords": [
    "eshop",
    "switch",
    "nintendo",
    "crawler",
    "lib",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lmmfranco/nintendo-switch-eshop.git"
  },
  "bugs": {
    "url": "https://github.com/lmmfranco/nintendo-switch-eshop/issues"
  },
  "lint-staged": {
    "*.{mjs,js,ts}": "eslint --fix --ext mjs,js,ts"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "resolutions": {
    "lodash": "^4.17.21",
    "dot-prop": "^6.0.1"
  }
}
